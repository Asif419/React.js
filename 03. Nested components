import React from 'react'
import ReactDom from 'react-dom'


function Greeting() {
  return (
    <div>
      <Name />
      <Welcome />
    </div>
  )
}

const Name = () => {
  return <h2>Asif</h2>
};
const Welcome = () => {
  return <h3>Welcome here</h3>
}
ReactDom.render(<Greeting></Greeting>, document.getElementById('root'));
